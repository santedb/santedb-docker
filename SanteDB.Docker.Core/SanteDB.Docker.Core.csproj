<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>2.1.3</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Features\Config\AtnaAuditFeature.xml" />
    <None Remove="Features\Config\DiagnosticsFeature.xml" />
    <None Remove="Features\Config\MatchingFeature.xml" />
    <None Remove="Features\Config\MemCacheFeature.xml" />
    <None Remove="Features\Config\RedisCacheFeature.xml" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Features\Config\AtnaAuditFeature.xml" />
    <EmbeddedResource Include="Features\Config\DiagnosticsFeature.xml" />
    <EmbeddedResource Include="Features\Config\MemCacheFeature.xml" />
    <EmbeddedResource Include="Features\Config\RedisCacheFeature.xml" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Features\Config\MatchingFeature.xml" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\SanteDB.licenseheader" Link="SanteDB.licenseheader" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SanteDB.Caching.Memory" Version="2.1.3" />
    <PackageReference Include="SanteDB.Caching.Redis" Version="2.1.3" />
    <PackageReference Include="SanteDB.Core.Api" Version="2.1.3" />
    <PackageReference Include="SanteDB.Matcher" Version="2.1.3" />
  </ItemGroup>

</Project>
